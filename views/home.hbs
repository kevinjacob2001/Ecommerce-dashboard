<table class="table">
  <thead class="">
    <tr>
      <th scope="col">#</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col" class="text-end">Action</th>
    </tr>
  </thead>
  <tbody>

  {{#each rows}}
    
    <tr>
      <th scope="row">{{this.id}}</th>
      <td>{{this.first_name}}</td>
      <td>{{this.last_name}}</td>
      <td>{{this.email}}</td>
      <td>{{this.phone}}</td>
      <td class="text-end">
          <a href="#" type="button" class="btn btn-light btn-small btn-view">View</a>
          <a href="#" type="button" class="btn btn-light btn-small btn-edit">Edit</a>
          <a href="#" type="button" class="btn btn-light btn-small btn-delete">Delete</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>